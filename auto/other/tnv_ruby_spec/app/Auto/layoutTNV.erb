<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <title>Automation</title>
  <meta name="viewport" content="initial-scale=1.0,maximum-scale=1,user-scalable=yes,width=device-width"/>
  <script src="/public/jquery/jquery-1.6.4.min.js" type="text/javascript"></script>
  <style>
  #detailsdiv {
    background: url("/public/images/auto/grid2.gif") repeat scroll 0 0 transparent;
    border: 1px solid #DCDCDC;
    color: #8B0000;
    display: block;
    font-weight: bold;
    left: 50px;
    padding: 10px;
    position: fixed;
    text-align: center;
    top: 200px;
    width: 250px;
  }
  
#manualdiv {
  background: url("/public/images/auto/grid2.gif") repeat scroll 0 0 transparent;
  border: 1px solid #DCDCDC;
  color: #8B0000;
  display: block;
  font-weight: bold;
  left: 0px;
  padding: 10px;
  position: fixed;
  text-align: center;
  bottom:0px;
  width: 100%;
}

#scannerparams {
  top: 200px;
}
  
  </style>
  <link rel="stylesheet" type="text/css" href="/public/jasmine-1.3.1/jasmine.css">
  <script type="text/javascript" src="/public/jasmine-1.3.1/jasmine.js"></script>
  <script type="text/javascript" src="/public/jasmine-1.3.1/jasmine-html.js"></script>

  <% if $mode == 'ATS' %>
    <!-- include Params files here...-->
    <!-- RE APIS -->
    <script src="/public/params/batteryParams.js" type="text/javascript"></script>
    <script src="/public/params/signalParams.js" type="text/javascript"></script>
    <script src="/public/params/fileTransferParams.js" type="text/javascript"></script>
    <script src="/public/params/deviceAndAppsParams.js" type="text/javascript"></script>
    <script src="/public/params/gestureParams.js" type="text/javascript"></script>
    <script src="/public/params/imagerParams.js" type="text/javascript"></script>
    <script src="/public/params/keyCaptureParams.js" type="text/javascript"></script>
    <script src="/public/params/signatureCaptureParams.js" type="text/javascript"></script>

    <!-- COMMON APIS -->
    <script src="/public/params/scannerParams.js" type="text/javascript"></script>
    <script src="/public/params/systemParams.js" type="text/javascript"></script>
    <script src="/public/params/scannerManualParams.js" type="text/javascript"></script>
    
    <!-- include spec files here...-->
    <script src="/public/spec/commonSpec.js" type="text/javascript"></script>
    <!-- RE APIS -->
    <script src="/public/spec/batterySpec.js" type="text/javascript"></script>
    <script src="/public/spec/signalSpec.js" type="text/javascript"></script>
    <script src="/public/spec/fileTransferSpec.js" type="text/javascript"></script>
    <script src="/public/spec/deviceAndAppsSpec.js" type="text/javascript"></script>
    <script src="/public/spec/gestureSpec.js" type="text/javascript"></script>
    <script src="/public/spec/imagerSpec.js" type="text/javascript"></script>
    <script src="/public/spec/keyCaptureSpec.js" type="text/javascript"></script>
    <script src="/public/spec/signatureCaptureSpec.js" type="text/javascript"></script>
    <!-- COMMON APIS -->
    <script src="/public/spec/systemSpec.js" type="text/javascript"></script>
    <script src="/public/spec/scannerManualSpec.js" type="text/javascript"></script>
  <% else %>
    <!-- include Params files here...-->
    <script src="http://192.168.6.27/jasmine/native/params/systemParams.js" type="text/javascript"></script>
    <% if $module == 'decodersetproperty' %>
    <script src="http://192.168.6.27/jasmine/native/params/scannerManualParams.js" type="text/javascript"></script>
    <% elsif $module == 'decodersetproperties' %>
    <script src="http://192.168.6.27/jasmine/native/params/decodersetPropertiesParams.js" type="text/javascript"></script>
    <% elsif $module == 'decoderdirect' %>
    <script src="http://192.168.6.27/jasmine/native/params/decoderDirectParams.js" type="text/javascript"></script>
    <% elsif $module == 'decoderenable' %>
    <script src="http://192.168.6.27/jasmine/native/params/decoderEnableParams.js" type="text/javascript"></script>
    <%end%>

    <% if $module == 'scannersetproperty' %>
    <script src="http://192.168.6.27/jasmine/native/params/scannerParams.js" type="text/javascript"></script>
    <% elsif $module == 'scannersetproperties' %>
    <script src="http://192.168.6.27/jasmine/native/params/scannerParamsSetProperties.js" type="text/javascript"></script>
    <% elsif $module == 'scannerdirect' %>
    <script src="http://192.168.6.27/jasmine/native/params/scannerParamsDirectly.js" type="text/javascript"></script>
    <% elsif $module == 'scannerenable' %>
    <script src="http://192.168.6.27/jasmine/native/params/scannerParamsSetEnable.js" type="text/javascript"></script>
    <%end%>
<script src="http://192.168.6.27/jasmine/native/params/databaseParams.js" type="text/javascript"></script>
    <!-- include spec files here...-->
    <script src="http://192.168.6.27/jasmine/native/spec/commonSpec.js" type="text/javascript"></script>
    <script src="http://192.168.6.27/jasmine/native/spec/scannerManualSpec.js" type="text/javascript"></script>
    <script src="http://192.168.6.27/jasmine/native/spec/systemSpec.js" type="text/javascript"></script>
    <script src="http://192.168.6.27/jasmine/native/spec/databaseSpec.js" type="text/javascript"></script>

  <%end%>

</head>
<body>
  <%= @content %>
<div id="detailsdiv"></div>
<br/><br/>
<div id="test">
<ul id="myList"></ul>
<ul id="myList2"></ul>
</div>
<br/><br/>
</body>
</html>