<?xml version = "1.0"?>
<?xml-stylesheet type="text/xsl" href="pb_help.xsl"?>
<API>
    <MODULE name="Dummy" parent="Rho">
        <HELP_OVERVIEW>Example extension api</HELP_OVERVIEW>
        <MORE_HELP>This is example of API. Implementation contain in extension.</MORE_HELP>
    
        <TEMPLATES>
            <DEFAULT_INSTANCE/>
            <PROPERTY_BAG/>
        </TEMPLATES>

         <PROPERTIES >
            <DESC>list of properties supported by instance of object</DESC>
            <PROPERTY name="simpleStringProperty" type="STRING" usePropertyBag="accessorsViaPropertyBag" >
                <DESC>simple string property</DESC>
            </PROPERTY>
        </PROPERTIES>

        <METHODS>

            <METHOD name="enumerate" access="STATIC" hasCallback="optional">
                <RETURN type="ARRAY">
                    <DESC>Array of Dummy objects</DESC>
                    <PARAM type="SELF_INSTANCE"/>
                </RETURN>
            </METHOD>
    
            <METHOD name="getPlatformName">
                <DESC>return string with platform</DESC>
                <RETURN type="STRING"/>
            </METHOD>
    
            <METHOD name="calcSumm">
                <DESC>return summ of two params: a+b</DESC>
                <PARAMS>
                    <PARAM name="a" type="INTEGER">
                    </PARAM>
                    <PARAM name="b" type="INTEGER">
                    </PARAM>
                </PARAMS>
                <RETURN type="INTEGER"/>
            </METHOD>

            <METHOD name="joinStrings">
                <DESC>return join of two strings: a+b</DESC>
                <PARAMS>
                    <PARAM name="a" type="STRING">
                    </PARAM>
                    <PARAM name="b" type="STRING">
                    </PARAM>
                </PARAMS>
                <RETURN type="STRING"/>
            </METHOD>
            
             <METHOD name="enable" hasCallback="mandatory">
                <PLATFORM>Android</PLATFORM>
                <DESC>Start firing in</DESC>
                <PARAMS>
                    <PARAM name="firingInterval" type="INTEGER">
                    </PARAM>
                </PARAMS>
                <CALLBACK type="HASH">
                    <DESC>Provides callback value</DESC>
                    <PARAMS>
                        <PARAM name="status" type="STRING">
                            <DESC>Always OK</DESC>
                        </PARAM>
                        <PARAM name="data" type="STRING">
                            <DESC>Always 9</DESC>
                        </PARAM>
                        
                    </PARAMS>
                </CALLBACK>
            </METHOD>
            
            <METHOD name="enable_Int" hasCallback="mandatory">
                <PLATFORM>Android</PLATFORM>
                <DESC>Start firing in</DESC>
                <PARAMS>
                    <PARAM name="firingInterval" type="INTEGER">
                    </PARAM>
                </PARAMS>
                <CALLBACK type="INTEGER">
                    <DESC>Provides callback value</DESC>
                </CALLBACK>
            </METHOD>
            
             <METHOD name="enable_str" hasCallback="mandatory">
                <PLATFORM>Android</PLATFORM>
                <DESC>Start firing in</DESC>
                <PARAMS>
                    <PARAM name="firingInterval" type="INTEGER">
                    </PARAM>
                </PARAMS>
                <CALLBACK type="STRING">
                    <DESC>Provides callback value</DESC>
                </CALLBACK>
            </METHOD>
            
            <METHOD name="enable_bool" hasCallback="mandatory">
                <PLATFORM>Android</PLATFORM>
                <DESC>Start firing in</DESC>
                <PARAMS>
                    <PARAM name="firingInterval" type="INTEGER">
                    </PARAM>
                </PARAMS>
                <CALLBACK type="BOOLEAN">
                    <DESC>Provides callback value</DESC>
                </CALLBACK>
            </METHOD>
            
            <METHOD name="enable_double" hasCallback="mandatory">
                <PLATFORM>Android</PLATFORM>
                <DESC>Start firing in</DESC>
                <PARAMS>
                    <PARAM name="firingInterval" type="INTEGER">
                    </PARAM>
                </PARAMS>
                <CALLBACK type="DOUBLE">
                    <DESC>Provides callback value</DESC>
                </CALLBACK>
            </METHOD>
            
            <METHOD name="stop" hasCallback="mandatory">
                <PLATFORM>Android</PLATFORM>
                <DESC>stop firing from enable</DESC>
             </METHOD>

        </METHODS>
        
        <USER_OVERVIEW>
        </USER_OVERVIEW>

        <VER_INTRODUCED>1.0.0</VER_INTRODUCED>
        <PLATFORM>
         </PLATFORM>
    </MODULE>
</API>    
