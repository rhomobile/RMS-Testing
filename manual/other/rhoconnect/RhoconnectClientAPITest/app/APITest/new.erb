<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @apitest.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @apitest.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="apitest[TestNumber]" class="fieldLabel">Testnumber</label>
            <input type="text" id="apitest[TestNumber]" name="apitest[TestNumber]" <%= placeholder("Testnumber") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
