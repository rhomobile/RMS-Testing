<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @cameraimage.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @cameraimage.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="cameraimage[serial_no]" class="fieldLabel">Serial no</label>
            <input type="text" id="cameraimage[serial_no]" name="cameraimage[serial_no]" <%= placeholder("Serial no") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
