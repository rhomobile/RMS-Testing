<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @canda.CandAModel %></h1>
    <a href="<%= url_for :action => :show, :id => @canda.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @canda.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @canda.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="canda[CandAModel]" class="fieldLabel">Candamodel</label>
            <input type="text" id="canda[CandAModel]" name="canda[CandAModel]" value="<%= @canda.CandAModel %>" <%= placeholder( "Candamodel" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
