}
function topSIP(){
  var topSip = document.getElementById('topSip').value;
  $.get('/app/Deviceapp/topSIP', { top: topSip
      });
    return false;
}
function setInterval(){
  var intervalTimer = document.getElementById('intervalTimer').value;
  $.get('/app/Deviceapp/setInterval', { interval: intervalTimer
