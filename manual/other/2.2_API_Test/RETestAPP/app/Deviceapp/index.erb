    return false;
}
function manageVolume(){
  var volumeValue = document.getElementById('volume').value;
  $.get('/app/Deviceapp/manageVolume', { volume: volumeValue
      });
    return false;
}
function setAlarmTime(){
  
  var objYear = document.getElementById('cboyear');
  var year = objYear.options[objYear.selectedIndex].value;
  objMonth = document.getElementById('cbomonth');
  var month = objMonth.options[objMonth.selectedIndex].value;
  objDay = document.getElementById('cboday');
  var day = objDay.options[objDay.selectedIndex].value;
  objHour = document.getElementById('cbohour');
  var hour = objHour.options[objHour.selectedIndex].value;
  objMin = document.getElementById('cbomin');
  var min = objMin.options[objMin.selectedIndex].value;

  objZone = document.getElementById('DropDownTimezone');
  var timeZone = objZone.options[objZone.selectedIndex].value;
  //var sTime = year+'-'+month+'-'+day+'t'+hour+'-'+min+'-00'+timeZone;
  var sTime = year+'-'+month+'-'+day+'t'+hour+'-'+min+'-00-00-00'; 

  $.get('/app/Deviceapp/setAlarmTime', { time: sTime
      });
    return false;
}

function funsetLocal(){
  var setLocal = document.getElementById('setLocal').value;
  $.get('/app/Deviceapp/funsetLocal', { setLocal: setLocal
      });
    return false;
}
function funsetUtc(){
  var setUtc = document.getElementById('setUtc').value;
  $.get('/app/Deviceapp/funsetUtc', { setUtc: setUtc
      });
