function setsntpServerIp(){
  var sntpServerIp = document.getElementById('sntpServerIp').value;
  $.get('/app/Deviceapp/setsntpServerIp', { sntpServerIp: sntpServerIp
      });
    return false;
}


function changeDivContent(divId,content)
{
 
  if(divId != "")
    divId.innerHTML = content
}

