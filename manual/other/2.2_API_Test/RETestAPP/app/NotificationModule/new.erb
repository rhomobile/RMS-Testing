<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @notificationmodule.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @notificationmodule.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="notificationmodule[NotificationModel]" class="fieldLabel">Notificationmodel</label>
            <input type="text" id="notificationmodule[NotificationModel]" name="notificationmodule[NotificationModel]" <%= placeholder("Notificationmodel") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
