<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @notificationmodule.NotificationModel %></h1>
    <a href="<%= url_for :action => :show, :id => @notificationmodule.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @notificationmodule.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @notificationmodule.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="notificationmodule[NotificationModel]" class="fieldLabel">Notificationmodel</label>
            <input type="text" id="notificationmodule[NotificationModel]" name="notificationmodule[NotificationModel]" value="<%= @notificationmodule.NotificationModel %>" <%= placeholder( "Notificationmodel" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
