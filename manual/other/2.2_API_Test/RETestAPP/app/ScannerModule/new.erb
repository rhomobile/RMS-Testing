<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @scannermodule.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @scannermodule.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="scannermodule[ScannerModel]" class="fieldLabel">Scannermodel</label>
            <input type="text" id="scannermodule[ScannerModel]" name="scannermodule[ScannerModel]" <%= placeholder("Scannermodel") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
