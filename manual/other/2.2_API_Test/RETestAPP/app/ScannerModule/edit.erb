<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @scannermodule.ScannerModel %></h1>
    <a href="<%= url_for :action => :show, :id => @scannermodule.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @scannermodule.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @scannermodule.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="scannermodule[ScannerModel]" class="fieldLabel">Scannermodel</label>
            <input type="text" id="scannermodule[ScannerModel]" name="scannermodule[ScannerModel]" value="<%= @scannermodule.ScannerModel %>" <%= placeholder( "Scannermodel" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
