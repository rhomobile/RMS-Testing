<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @configurationmodule.ConfigurationModel %></h1>
    <a href="<%= url_for :action => :show, :id => @configurationmodule.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @configurationmodule.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @configurationmodule.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="configurationmodule[ConfigurationModel]" class="fieldLabel">Configurationmodel</label>
            <input type="text" id="configurationmodule[ConfigurationModel]" name="configurationmodule[ConfigurationModel]" value="<%= @configurationmodule.ConfigurationModel %>" <%= placeholder( "Configurationmodel" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
