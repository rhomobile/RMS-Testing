<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @configurationmodule.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @configurationmodule.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="configurationmodule[ConfigurationModel]" class="fieldLabel">Configurationmodel</label>
            <input type="text" id="configurationmodule[ConfigurationModel]" name="configurationmodule[ConfigurationModel]" <%= placeholder("Configurationmodel") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
