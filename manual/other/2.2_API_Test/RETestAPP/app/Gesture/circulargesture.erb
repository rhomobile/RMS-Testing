 }
  function circularGesture(){
  var txtCircleID = document.getElementById('txtCircleID').value;
  var cmdPreset = document.getElementById('cmdPreset').value;
  var cmdDiagonstics = document.getElementById('cmdDiagonstics').value;
  var Text1 = document.getElementById('Text1').value;
  var Text2 = document.getElementById('Text2').value;
  var Text3 = document.getElementById('Text3').value;
  var txtstart = document.getElementById('txtstart').value;
  var txtend = document.getElementById('txtend').value;
  var txttolerance = document.getElementById('txttolerance').value;
  var txtsensitivity = document.getElementById('txtsensitivity').value;
  var txtType = document.getElementById('txtType').value;
  var txtNumber = document.getElementById('txtNumber').value;

  $.get('/app/Gesture/performCircularGesture', { 
    formType: txtType,
    formId: txtCircleID,
    formPreset: cmdPreset,
    formDiagnostics: cmdDiagonstics,
    formCenterX: Text1,
    formCenterY: Text2,
    formRadius: Text3,
    formStart: txtstart,
    formEnd: txtend,
    formTolerance: txttolerance,
    formSensitivity: txtsensitivity,
    formTotalNo: txtNumber
    });
  return false;
}

function setFieldValue(returnval) 
{
  document.getElementById("myResult").innerHTML=returnval;
}
</script>

