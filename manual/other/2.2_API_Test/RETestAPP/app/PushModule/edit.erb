<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @pushmodule.PushModel %></h1>
    <a href="<%= url_for :action => :show, :id => @pushmodule.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @pushmodule.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @pushmodule.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="pushmodule[PushModel]" class="fieldLabel">Pushmodel</label>
            <input type="text" id="pushmodule[PushModel]" name="pushmodule[PushModel]" value="<%= @pushmodule.PushModel %>" <%= placeholder( "Pushmodel" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
