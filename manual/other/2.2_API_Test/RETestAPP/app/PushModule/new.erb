<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @pushmodule.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @pushmodule.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="pushmodule[PushModel]" class="fieldLabel">Pushmodel</label>
            <input type="text" id="pushmodule[PushModel]" name="pushmodule[PushModel]" <%= placeholder("Pushmodel") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
