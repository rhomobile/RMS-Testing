<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @lan.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @lan.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="lan[city]" class="fieldLabel">City</label>
            <input type="text" id="lan[city]" name="lan[city]" <%= placeholder("City") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="lan[name]" class="fieldLabel">Name</label>
            <input type="text" id="lan[name]" name="lan[name]" <%= placeholder("Name") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
