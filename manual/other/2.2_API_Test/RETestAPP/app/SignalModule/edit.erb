<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @signalmodule.SignalModel %></h1>
    <a href="<%= url_for :action => :show, :id => @signalmodule.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @signalmodule.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @signalmodule.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="signalmodule[SignalModel]" class="fieldLabel">Signalmodel</label>
            <input type="text" id="signalmodule[SignalModel]" name="signalmodule[SignalModel]" value="<%= @signalmodule.SignalModel %>" <%= placeholder( "Signalmodel" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
