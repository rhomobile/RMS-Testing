<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @rsm.RSMattribute %></h1>
    <a href="<%= url_for :action => :show, :id => @rsm.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @rsm.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @rsm.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="rsm[RSMattribute]" class="fieldLabel">Rsmattribute</label>
            <input type="text" id="rsm[RSMattribute]" name="rsm[RSMattribute]" value="<%= @rsm.RSMattribute %>" <%= placeholder( "Rsmattribute" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
