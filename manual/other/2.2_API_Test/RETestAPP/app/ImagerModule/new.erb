<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @imagermodule.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @imagermodule.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="imagermodule[ImagerName]" class="fieldLabel">Imagername</label>
            <input type="text" id="imagermodule[ImagerName]" name="imagermodule[ImagerName]" <%= placeholder("Imagername") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
