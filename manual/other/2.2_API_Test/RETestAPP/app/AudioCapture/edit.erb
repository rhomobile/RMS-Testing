<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @audiocapture.audioName %></h1>
    <a href="<%= url_for :action => :show, :id => @audiocapture.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @audiocapture.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @audiocapture.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="audiocapture[audioName]" class="fieldLabel">Audioname</label>
            <input type="text" id="audiocapture[audioName]" name="audiocapture[audioName]" value="<%= @audiocapture.audioName %>" <%= placeholder( "Audioname" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
