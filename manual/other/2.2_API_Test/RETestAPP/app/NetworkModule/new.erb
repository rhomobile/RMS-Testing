<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @networkmodule.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @networkmodule.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="networkmodule[NetworkModel]" class="fieldLabel">Networkmodel</label>
            <input type="text" id="networkmodule[NetworkModel]" name="networkmodule[NetworkModel]" <%= placeholder("Networkmodel") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
