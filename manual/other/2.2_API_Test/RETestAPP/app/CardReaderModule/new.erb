<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @cardreadermodule.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @cardreadermodule.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="cardreadermodule[CardReaderModel]" class="fieldLabel">Cardreadermodel</label>
            <input type="text" id="cardreadermodule[CardReaderModel]" name="cardreadermodule[CardReaderModel]" <%= placeholder("Cardreadermodel") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
