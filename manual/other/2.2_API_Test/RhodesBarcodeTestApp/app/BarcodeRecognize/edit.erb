<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @barcoderecognize.testid %></h1>
    <a href="<%= url_for :action => :show, :id => @barcoderecognize.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @barcoderecognize.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @barcoderecognize.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="barcoderecognize[testid]" class="fieldLabel">Testid</label>
            <input type="text" id="barcoderecognize[testid]" name="barcoderecognize[testid]" value="<%= @barcoderecognize.testid %>" <%= placeholder( "Testid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="barcoderecognize[name]" class="fieldLabel">Name</label>
            <input type="text" id="barcoderecognize[name]" name="barcoderecognize[name]" value="<%= @barcoderecognize.name %>" <%= placeholder( "Name" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="barcoderecognize[expected]" class="fieldLabel">Expected</label>
            <input type="text" id="barcoderecognize[expected]" name="barcoderecognize[expected]" value="<%= @barcoderecognize.expected %>" <%= placeholder( "Expected" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="barcoderecognize[output]" class="fieldLabel">Output</label>
            <input type="text" id="barcoderecognize[output]" name="barcoderecognize[output]" value="<%= @barcoderecognize.output %>" <%= placeholder( "Output" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="barcoderecognize[result]" class="fieldLabel">Result</label>
            <input type="text" id="barcoderecognize[result]" name="barcoderecognize[result]" value="<%= @barcoderecognize.result %>" <%= placeholder( "Result" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
