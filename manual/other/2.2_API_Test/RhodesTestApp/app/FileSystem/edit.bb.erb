<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @filesystem.object %>
    <h1>Edit <%= @filesystem.testcaseid%></h1>
    <%= link_to "Delete", :action => :delete, :id => @filesystem.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @filesystem.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Testcaseid: </td>
              <td class="itemValue"><input type="text" name="filesystem[testcaseid]" value="<%= @filesystem.testcaseid %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Description: </td>
              <td class="itemValue"><input type="text" name="filesystem[description]" value="<%= @filesystem.description %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res1: </td>
              <td class="itemValue"><input type="text" name="filesystem[res1]" value="<%= @filesystem.res1 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res2: </td>
              <td class="itemValue"><input type="text" name="filesystem[res2]" value="<%= @filesystem.res2 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res3: </td>
              <td class="itemValue"><input type="text" name="filesystem[res3]" value="<%= @filesystem.res3 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res4: </td>
              <td class="itemValue"><input type="text" name="filesystem[res4]" value="<%= @filesystem.res4 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res5: </td>
              <td class="itemValue"><input type="text" name="filesystem[res5]" value="<%= @filesystem.res5 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res6: </td>
              <td class="itemValue"><input type="text" name="filesystem[res6]" value="<%= @filesystem.res6 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res7: </td>
              <td class="itemValue"><input type="text" name="filesystem[res7]" value="<%= @filesystem.res7 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res8: </td>
              <td class="itemValue"><input type="text" name="filesystem[res8]" value="<%= @filesystem.res8 %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
