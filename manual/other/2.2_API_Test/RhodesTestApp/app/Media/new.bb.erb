<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Media</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @media.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Testcaseid: </td>
                <td class="itemValue"><input type="text" name="media[testcaseid]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Description: </td>
                <td class="itemValue"><input type="text" name="media[description]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Res1: </td>
                <td class="itemValue"><input type="text" name="media[res1]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Res2: </td>
                <td class="itemValue"><input type="text" name="media[res2]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Res3: </td>
                <td class="itemValue"><input type="text" name="media[res3]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Res4: </td>
                <td class="itemValue"><input type="text" name="media[res4]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Res5: </td>
                <td class="itemValue"><input type="text" name="media[res5]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Res6: </td>
                <td class="itemValue"><input type="text" name="media[res6]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Res7: </td>
                <td class="itemValue"><input type="text" name="media[res7]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Res8: </td>
                <td class="itemValue"><input type="text" name="media[res8]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
