<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @calendar.object %>
    <h1>Edit <%= @calendar.testcaseid%></h1>
    <%= link_to "Delete", :action => :delete, :id => @calendar.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @calendar.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Testcaseid: </td>
              <td class="itemValue"><input type="text" name="calendar[testcaseid]" value="<%= @calendar.testcaseid %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Description: </td>
              <td class="itemValue"><input type="text" name="calendar[description]" value="<%= @calendar.description %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res1: </td>
              <td class="itemValue"><input type="text" name="calendar[res1]" value="<%= @calendar.res1 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res2: </td>
              <td class="itemValue"><input type="text" name="calendar[res2]" value="<%= @calendar.res2 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res3: </td>
              <td class="itemValue"><input type="text" name="calendar[res3]" value="<%= @calendar.res3 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res4: </td>
              <td class="itemValue"><input type="text" name="calendar[res4]" value="<%= @calendar.res4 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res5: </td>
              <td class="itemValue"><input type="text" name="calendar[res5]" value="<%= @calendar.res5 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res6: </td>
              <td class="itemValue"><input type="text" name="calendar[res6]" value="<%= @calendar.res6 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res7: </td>
              <td class="itemValue"><input type="text" name="calendar[res7]" value="<%= @calendar.res7 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res8: </td>
              <td class="itemValue"><input type="text" name="calendar[res8]" value="<%= @calendar.res8 %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
