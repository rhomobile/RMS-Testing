              <td class="itemValue"><input type="text" name="generatepdf[res1]" value="<%= @generatepdf.res1 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res2: </td>
              <td class="itemValue"><input type="text" name="generatepdf[res2]" value="<%= @generatepdf.res2 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res3: </td>
              <td class="itemValue"><input type="text" name="generatepdf[res3]" value="<%= @generatepdf.res3 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res4: </td>
              <td class="itemValue"><input type="text" name="generatepdf[res4]" value="<%= @generatepdf.res4 %>"/></td>
