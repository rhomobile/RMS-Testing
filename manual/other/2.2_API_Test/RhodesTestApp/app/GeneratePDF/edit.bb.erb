          <tr>
              <td class="itemLabel">Res7: </td>
              <td class="itemValue"><input type="text" name="generatepdf[res7]" value="<%= @generatepdf.res7 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res8: </td>
              <td class="itemValue"><input type="text" name="generatepdf[res8]" value="<%= @generatepdf.res8 %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
