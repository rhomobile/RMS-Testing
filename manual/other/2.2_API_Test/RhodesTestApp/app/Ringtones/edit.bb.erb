<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @ringtones.object %>
    <h1>Edit <%= @ringtones.testcaseid%></h1>
    <%= link_to "Delete", :action => :delete, :id => @ringtones.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @ringtones.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Testcaseid: </td>
              <td class="itemValue"><input type="text" name="ringtones[testcaseid]" value="<%= @ringtones.testcaseid %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Description: </td>
              <td class="itemValue"><input type="text" name="ringtones[description]" value="<%= @ringtones.description %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res1: </td>
              <td class="itemValue"><input type="text" name="ringtones[res1]" value="<%= @ringtones.res1 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res2: </td>
              <td class="itemValue"><input type="text" name="ringtones[res2]" value="<%= @ringtones.res2 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res3: </td>
              <td class="itemValue"><input type="text" name="ringtones[res3]" value="<%= @ringtones.res3 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res4: </td>
              <td class="itemValue"><input type="text" name="ringtones[res4]" value="<%= @ringtones.res4 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res5: </td>
              <td class="itemValue"><input type="text" name="ringtones[res5]" value="<%= @ringtones.res5 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res6: </td>
              <td class="itemValue"><input type="text" name="ringtones[res6]" value="<%= @ringtones.res6 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res7: </td>
              <td class="itemValue"><input type="text" name="ringtones[res7]" value="<%= @ringtones.res7 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Res8: </td>
              <td class="itemValue"><input type="text" name="ringtones[res8]" value="<%= @ringtones.res8 %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
