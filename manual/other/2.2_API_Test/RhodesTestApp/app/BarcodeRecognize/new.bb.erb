<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New BarcodeRecognize</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @barcoderecognize.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Testid: </td>
                <td class="itemValue"><input type="text" name="barcoderecognize[testid]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Name: </td>
                <td class="itemValue"><input type="text" name="barcoderecognize[name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Expected: </td>
                <td class="itemValue"><input type="text" name="barcoderecognize[expected]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Output: </td>
                <td class="itemValue"><input type="text" name="barcoderecognize[output]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Result: </td>
                <td class="itemValue"><input type="text" name="barcoderecognize[result]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
