<script type="text/javascript" src="/public/jquery/jquery-1.6.4.min.js"></script>
<script type="text/javascript">
function setFieldValue(returnval) 
{
  document.getElementById("myResult").innerHTML=returnval;
}

function connectionDetect() {
  var hostText=document.getElementById("hostText").value;
  var portText=document.getElementById("portText").value;
  var pollIntervalText=document.getElementById("pollIntervalText").value;
  var detectionTimeoutText=document.getElementById("detectionTimeoutText").value;
  $.get('/app/Network/connectionDetect', { hostText:  hostText, portText: portText, pollIntervalText: pollIntervalText, detectionTimeoutText: detectionTimeoutText });
  return false;
}
function wanconnectionDestination() {
  var wanDestinationText=document.getElementById("wanDestinationText").value;
  $.get('/app/Network/wanconnectionDestination', { wanDestinationText:  wanDestinationText});
  return false;
}

</script>

